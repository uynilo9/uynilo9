    "@iconify-json/simple-icons": "^1.2.2",
    "@iconify-json/tabler": "^1.1.105",
    "@iconify-json/twemoji": "^1.1.15",
    "@iconify-json/vscode-icons": "^1.1.33",
    "@vitejs/plugin-vue": "^4.5.2",
    "bun-types": "latest",
    "prettier": "3.3.2",
    "sass": "^1.78.0",
    "typescript": "^5.2.2",
    "unocss": "^0.58.4",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
